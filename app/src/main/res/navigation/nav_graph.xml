<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/mobile_nav"
	app:startDestination="@id/movieInfoFragment">
	<fragment
		android:id="@+id/movieInfoFragment"
		android:name="com.rsdev.mymovies.view.ui.movieInfo.MovieInfoFragment"
		android:label="Movie List"
		tools:layout="@layout/movie_info_fragment">
		<action
			android:id="@+id/action_movieInfoFragment_to_movieDetailsFragment"
			app:destination="@id/movieDetailsFragment" />
	</fragment>
	<fragment
		android:id="@+id/movieDetailsFragment"
		android:name="com.rsdev.mymovies.view.ui.moviedetails.MovieDetailFragment"
		tools:layout="@layout/movie_detail_fragment" />


</navigation>